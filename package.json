{
    "name": "aeon",
    "productName": "Aeon",
    "version": "0.1.0-alpha8",
    "description": "Online identity versioning for the masses",
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx .",
        "test": "mocha --exit --bail",
        "preinstall": "npx npm-force-resolutions",
        "postinstall": "node scripts/prepareNodeGit.js"
    },
    "keywords": [],
    "author": {
        "name": "Lei Nelissen",
        "email": "lei@codified.nl"
    },
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/leinelissen/aeon"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./src/icon",
                "executableName": "aeon"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "iconUrl": "https://raw.githubusercontent.com/leinelissen/aeon/master/src/icon.ico",
                        "setupIcon": "./src/icon.ico"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/app/index.ejs",
                                    "js": "./src/app/index.tsx",
                                    "name": "main_window"
                                }
                            ]
                        },
                        "loggerPort": 9001
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.53",
        "@electron-forge/maker-deb": "^6.0.0-beta.53",
        "@electron-forge/maker-rpm": "^6.0.0-beta.53",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
        "@electron-forge/maker-zip": "^6.0.0-beta.53",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.53",
        "@types/adm-zip": "^0.4.33",
        "@types/deep-equal": "^1.0.1",
        "@types/lodash": "^4.14.162",
        "@types/node": "^12.12.61",
        "@types/node-fetch": "^2.5.7",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "@types/react-router": "^5.1.8",
        "@types/react-router-dom": "^5.1.6",
        "@types/source-map-support": "^0.5.3",
        "@types/styled-components": "^5.1.4",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "csp-html-webpack-plugin": "4.0.0",
        "css-loader": "^5.0.0",
        "dotenv-webpack": "4.0.0",
        "electron": "^10.1.3",
        "eslint": "^7.11.0",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-react": "^7.21.4",
        "file-loader": "^6.1.1",
        "fork-ts-checker-webpack-plugin": "^5.2.0",
        "mini-css-extract-plugin": "^1.0.0",
        "mocha": "^8.2.0",
        "node-abi": "^2.19.1",
        "node-loader": "^1.0.2",
        "react-hot-loader": "^4.13.0",
        "spectron": "^12.0.0",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.5",
        "typescript": "^4.0.3"
    },
    "dependencies": {
        "@fast-csv/parse": "^4.3.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-brands-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "@openfonts/ibm-plex-mono_all": "^1.44.2",
        "@openfonts/ibm-plex-sans_all": "^1.44.2",
        "@popperjs/core": "^2.5.3",
        "@types/nodegit": "^0.26.11",
        "adm-zip": "^0.4.16",
        "cogo-toast": "^4.2.3",
        "date-fns": "^2.16.1",
        "deep-equal": "^2.0.4",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^6.0.1",
        "hex-to-rgba": "^2.0.1",
        "keytar": "^7.0.0",
        "lodash": "^4.17.20",
        "node-fetch": "^2.6.1",
        "nodegit": "^0.27.0",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-popper": "^2.2.3",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-spring": "^8.0.27",
        "stream-chain": "^2.2.3",
        "styled-components": "^5.2.0",
        "undux": "^5.2.0",
        "v8-compile-cache": "^2.1.1"
    },
    "resolutions": {
        "normalize-url": "^4.5.0"
    }
}
