{
    "name": "aeon",
    "productName": "Aeon",
    "version": "0.2.8",
    "description": "Online identity versioning for the masses",
    "main": ".webpack/main",
    "sideEffects": [
        "src/app/components/App.tsx",
        "src/app/styles/index.ts",
        "src/main/lib/map-map.ts",
        "*.css",
        "*.svg",
        "*.woff",
        "*.woff2"
    ],
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx .",
        "test": "mocha --exit --bail",
        "preinstall": "npx npm-force-resolutions",
        "postinstall": "node scripts/prepareNodeGit.js"
    },
    "keywords": [],
    "author": {
        "name": "Lei Nelissen",
        "email": "lei@codified.nl"
    },
    "license": "EUPL-1.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/leinelissen/aeon"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./src/icon",
                "executableName": "aeon"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "iconUrl": "https://raw.githubusercontent.com/leinelissen/aeon/master/src/icon.ico",
                        "setupIcon": "./src/icon.ico"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/app/index.ejs",
                                    "js": "./src/app/index.tsx",
                                    "name": "main_window"
                                }
                            ]
                        },
                        "loggerPort": 9001
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-numeric-separator": "^7.12.13",
        "@babel/plugin-proposal-optional-chaining": "^7.13.8",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@electron-forge/cli": "^6.0.0-beta.54",
        "@electron-forge/maker-deb": "^6.0.0-beta.54",
        "@electron-forge/maker-dmg": "^6.0.0-beta.54",
        "@electron-forge/maker-rpm": "^6.0.0-beta.54",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
        "@electron-forge/maker-zip": "^6.0.0-beta.54",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
        "@types/adm-zip": "^0.4.33",
        "@types/cytoscape": "^3.14.12",
        "@types/deep-equal": "^1.0.1",
        "@types/lodash-es": "^4.17.4",
        "@types/mailparser": "^3.0.2",
        "@types/node": "^12.20.4",
        "@types/node-fetch": "^2.5.8",
        "@types/nodegit": "^0.26.15",
        "@types/nodemailer": "^6.4.1",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/react-redux": "^7.1.16",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-select": "^4.0.13",
        "@types/reactour": "^1.18.1",
        "@types/source-map-support": "^0.5.3",
        "@types/styled-components": "^5.1.9",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-styled-components": "^1.12.0",
        "csp-html-webpack-plugin": "5.1.0",
        "css-loader": "^5.1.3",
        "dotenv-webpack": "^5.0.0",
        "electron": "^13.1.2",
        "electron-devtools-installer": "^3.1.1",
        "eslint": "^7.22.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-react": "^7.22.0",
        "file-loader": "^6.2.0",
        "mini-css-extract-plugin": "^1.3.9",
        "mocha": "^8.3.2",
        "node-abi": "^2.21.0",
        "node-loader": "^1.0.2",
        "react-hot-loader": "^4.13.0",
        "spectron": "^12.0.0",
        "style-loader": "^2.0.0",
        "typescript": "^4.2.3",
        "webpack-bundle-analyzer": "^4.4.0"
    },
    "dependencies": {
        "@fast-csv/parse": "^4.3.6",
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "@openfonts/ibm-plex-mono_all": "^1.44.2",
        "@openfonts/ibm-plex-sans_all": "^1.44.2",
        "@popperjs/core": "^2.9.1",
        "@reduxjs/toolkit": "^1.5.0",
        "adm-zip": "^0.5.4",
        "cogo-toast": "^4.2.3",
        "cytoscape": "^3.18.1",
        "cytoscape-fcose": "^2.0.0",
        "date-fns": "^2.19.0",
        "deep-equal": "^2.0.5",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^6.0.1",
        "eventemitter2": "^6.4.4",
        "hex-to-rgba": "^2.0.1",
        "history": "^5.0.0",
        "keytar": "^7.4.0",
        "lodash-es": "^4.17.21",
        "mailparser": "^3.1.0",
        "node-fetch": "^2.6.1",
        "nodegit": "^0.27.0",
        "nodemailer": "^6.5.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-popper": "^2.2.4",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-select": "^4.3.0",
        "react-spring": "^8.0.27",
        "reactour": "^1.18.3",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "stream-chain": "^2.2.4",
        "styled-components": "^5.2.1",
        "v8-compile-cache": "^2.3.0"
    },
    "resolutions": {
        "normalize-url": "^4.5.0"
    }
}
