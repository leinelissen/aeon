{
    "name": "aeon",
    "productName": "Aeon",
    "version": "0.2.8",
    "description": "Online identity versioning for the masses",
    "main": ".webpack/main/index.js",
    "private": true,
    "sideEffects": [
        "src/app/components/App.tsx",
        "src/app/styles/index.ts",
        "src/main/lib/map-map.ts",
        "*.css",
        "*.svg",
        "*.woff",
        "*.woff2"
    ],
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx .",
        "test": "mocha --exit --bail",
        "postinstall": "npm run babel -w nodegit"
    },
    "keywords": [],
    "author": {
        "name": "Lei Nelissen",
        "email": "lei@codified.nl"
    },
    "license": "EUPL-1.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/leinelissen/aeon"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./src/icon",
                "executableName": "aeon"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "iconUrl": "https://raw.githubusercontent.com/leinelissen/aeon/master/src/icon.ico",
                        "setupIcon": "./src/icon.ico"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/app/index.ejs",
                                    "js": "./src/app/index.tsx",
                                    "name": "main_window"
                                }
                            ]
                        },
                        "loggerPort": 9001
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.61",
        "@electron-forge/maker-deb": "^6.0.0-beta.61",
        "@electron-forge/maker-dmg": "^6.0.0-beta.61",
        "@electron-forge/maker-rpm": "^6.0.0-beta.61",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
        "@electron-forge/maker-zip": "^6.0.0-beta.61",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
        "@swc/core": "^1.2.106",
        "@types/adm-zip": "^0.4.34",
        "@types/cytoscape": "^3.19.0",
        "@types/lodash-es": "^4.17.5",
        "@types/mailparser": "^3.0.3",
        "@types/node": "^16.11.6",
        "@types/node-fetch": "^3.0.2",
        "@types/nodegit": "^0.27.5",
        "@types/nodemailer": "^6.4.4",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-redux": "^7.1.20",
        "@types/react-router-dom": "^5.3.2",
        "@types/react-select": "^5.0.0",
        "@types/reactour": "^1.18.2",
        "@types/source-map-support": "^0.5.4",
        "@types/styled-components": "^5.1.15",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "@vercel/webpack-asset-relocator-loader": "^1.7.0",
        "babel-plugin-styled-components": "^1.13.3",
        "csp-html-webpack-plugin": "5.1.0",
        "css-loader": "^6.5.0",
        "dotenv-webpack": "^7.0.3",
        "electron": "^15.3.0",
        "electron-devtools-installer": "^3.2.0",
        "eslint": "^8.1.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-react": "^7.26.1",
        "file-loader": "^6.2.0",
        "mini-css-extract-plugin": "^2.4.3",
        "mocha": "^9.1.3",
        "node-abi": "^3.3.0",
        "node-loader": "^2.0.0",
        "react-hot-loader": "^4.13.0",
        "spectron": "^15.0.0",
        "style-loader": "^3.3.1",
        "swc-loader": "^0.1.15",
        "typescript": "^4.4.4",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "dependencies": {
        "@fast-csv/parse": "^4.3.6",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "@openfonts/ibm-plex-mono_all": "^1.44.2",
        "@openfonts/ibm-plex-sans_all": "^1.44.2",
        "@popperjs/core": "^2.10.2",
        "@reduxjs/toolkit": "^1.6.2",
        "adm-zip": "^0.5.9",
        "cogo-toast": "^4.2.3",
        "cytoscape": "^3.20.0",
        "cytoscape-fcose": "^2.1.0",
        "date-fns": "^2.25.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^8.0.1",
        "eventemitter2": "^6.4.5",
        "hex-to-rgba": "^2.0.1",
        "history": "^5.1.0",
        "keytar": "^7.7.0",
        "lodash-es": "^4.17.21",
        "mailparser": "^3.4.0",
        "node-fetch": "^3.0.0",
        "nodegit": "file:vendor/nodegit",
        "nodemailer": "^6.7.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-popper": "^2.2.5",
        "react-redux": "^7.2.6",
        "react-router-dom": "^5.3.0",
        "react-select": "^5.2.0",
        "react-spring": "^8.0.27",
        "reactour": "^1.18.6",
        "redux": "^4.1.2",
        "redux-persist": "^6.0.0",
        "stream-chain": "^2.2.4",
        "styled-components": "^5.3.3",
        "v8-compile-cache": "^2.3.0"
    },
    "workspaces": [
        "./vendor/nodegit"
    ]
}
